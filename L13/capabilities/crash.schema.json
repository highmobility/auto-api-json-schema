{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "incidents": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/crash_incident" },
        "type": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/type" },
        "tipped_state": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/tipped_state" },
        "automatic_ecall": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/enabled_state" },
        "severity": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/uinteger" },
        "impact_zone": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/impact_zone" },

        "nonce": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/nonce" },
        "vin": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/failure"
                },
                "availability": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/availability"
                }
            },
            "minProperties": 1
        },


        "property_crash_incident": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/crash_incident",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/crash_incident"
                }
            }
        },

        "property_enabled_state": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/enabled_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/enabled_state"
                }
            }
        },

        "property_impact_zone": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/impact_zone",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["pedestrian_protection", "rollover", "rear_passenger_side", "rear_driver_side", "side_passeger_side", "side_driver_side", "front_passenger_side", "front_driver_side"]
                }
            }
        },

        "property_nonce": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/nonce",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/bytes",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_tipped_state": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/tipped_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["tipped_over", "not_tipped"]
                }
            }
        },

        "property_type": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/type",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["pedestrian", "non_pedestrian"]
                }
            }
        },

        "property_uinteger": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/uinteger",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property",
            "properties": {
                "data": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                }
            }
        },

        "property_vin": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property/vin",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/crash.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
