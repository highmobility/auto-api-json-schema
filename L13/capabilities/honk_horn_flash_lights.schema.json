{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/honk_horn_flash_lights.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "flashers": { "$ref": "/schemas/property/flashers" },

        "nonce": { "$ref": "/schemas/property/nonce" },
        "vin": { "$ref": "/schemas/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "/schemas/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure"
                },
                "availability": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability"
                }
            },
            "minProperties": 1
        },


        "property_bytes": {
            "$id": "/schemas/property/bytes",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    }
                }
            }
        },

        "property_flashers": {
            "$id": "/schemas/property/flashers",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["inactive", "emergency_flasher_active", "left_flasher_active", "right_flasher_active"]
                }
            }
        },

        "property_nonce": {
            "$id": "/schemas/property/nonce",
            "$ref": "/schemas/property/bytes",
            "properties": {
                "data": {
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_vin": {
            "$id": "/schemas/property/vin",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
