{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "outside_light": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property/illuminance" },
        "inside_light": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property/illuminance" },

        "nonce": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property/nonce" },
        "vin": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/failure"
                },
                "availability": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/availability"
                }
            },
            "minProperties": 1
        },


        "property_illuminance": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property/illuminance",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/illuminance"
                }
            }
        },

        "property_nonce": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property/nonce",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property/bytes",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_vin": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property/vin",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/light_conditions.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
