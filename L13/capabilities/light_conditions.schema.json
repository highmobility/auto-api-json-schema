{
    "$id": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/all-properties-multiple/L13/capabilities/light_conditions.schema.json",
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "outside_light": {
            "description": "Measured outside illuminance",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_illuminance"
            }
        },
        "inside_light": {
            "description": "Measured inside illuminance",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_illuminance"
            }
        },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property_illuminance": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/units.schema.json#/$defs/illuminance"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_nonce": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_vin": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        }
    }
}
