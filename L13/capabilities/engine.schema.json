{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/capabilities/engine.schema.json",
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "status": {
            "description": "Status",
            "$ref": "#/$defs/property_on_off_state"
        },
        "start_stop_state": {
            "description": "Indicates wheter the start-stop system is currently active or not",
            "$ref": "#/$defs/property_active_state"
        },
        "start_stop_enabled": {
            "description": "Indicates if the automatic start-stop system is enabled or not",
            "$ref": "#/$defs/property_enabled_state"
        },
        "preconditioning_enabled": {
            "description": "Use of the engine pre-conditioning is enabled.",
            "$ref": "#/$defs/property_enabled_state"
        },
        "preconditioning_active": {
            "description": "Pre-conditioning is running.",
            "$ref": "#/$defs/property_active_state"
        },
        "preconditioning_remaining_time": {
            "description": "Remaining time of pre-conditioning.",
            "$ref": "#/$defs/property_duration"
        },
        "preconditioning_error": {
            "description": "Reason for not carrying out pre-conditioning.",
            "$ref": "#/$defs/property_preconditioning_error"
        },
        "preconditioning_status": {
            "description": "Status of the pre-conditioning system.",
            "$ref": "#/$defs/property_preconditioning_status"
        },
        "limp_mode": {
            "description": "Indicates wheter the engine is in fail-safe mode.",
            "$ref": "#/$defs/property_active_state"
        },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property_active_state": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/active_state"
                },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
        "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }.indented(byLevel: 2))
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_duration": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/duration"
                },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
        "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }.indented(byLevel: 2))
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_enabled_state": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/enabled_state"
                },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
        "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }.indented(byLevel: 2))
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_nonce": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_on_off_state": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/on_off_state"
                },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
        "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }.indented(byLevel: 2))
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_preconditioning_error": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["low_fuel", "low_battery", "quota_exceeded", "heater_failure", "component_failure", "open_or_unlocked", "ok"]
                },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
        "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }.indented(byLevel: 2))
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_preconditioning_status": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["standby", "heating", "cooling", "ventilation", "inactive"]
                },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
        "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }.indented(byLevel: 2))
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_vin": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        }
    }
}
