            },
            "minProperties": 1
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        },

        "property_volume": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/volume"
