{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/capabilities/usage.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "average_weekly_distance": { "$ref": "#/$defs/property_length" },
        "average_weekly_distance_long_run": { "$ref": "#/$defs/property_length" },
        "acceleration_evaluation": { "$ref": "#/$defs/property_percentage" },
        "driving_style_evaluation": { "$ref": "#/$defs/property_percentage" },
        "driving_modes_activation_periods": { "$ref": "#/$defs/property_driving_mode_activation_period" },
        "driving_modes_energy_consumptions": { "$ref": "#/$defs/property_driving_mode_energy_consumption" },
        "last_trip_energy_consumption": { "$ref": "#/$defs/property_energy" },
        "last_trip_fuel_consumption": { "$ref": "#/$defs/property_volume" },
        "mileage_after_last_trip": {
            "$ref": "#/$defs/property_length",
            "deprecated": true
        },
        "last_trip_electric_portion": { "$ref": "#/$defs/property_percentage" },
        "last_trip_average_energy_recuperation": { "$ref": "#/$defs/property_energy_efficiency" },
        "last_trip_battery_remaining": { "$ref": "#/$defs/property_percentage" },
        "last_trip_date": { "$ref": "#/$defs/property_timestamp" },
        "average_fuel_consumption": { "$ref": "#/$defs/property_fuel_efficiency" },
        "current_fuel_consumption": { "$ref": "#/$defs/property_fuel_efficiency" },
        "odometer_after_last_trip": { "$ref": "#/$defs/property_length" },
        "safety_driving_score": { "$ref": "#/$defs/property_percentage" },
        "rapid_acceleration_grade": { "$ref": "#/$defs/property_grade" },
        "rapid_deceleration_grade": { "$ref": "#/$defs/property_grade" },
        "late_night_grade": { "$ref": "#/$defs/property_grade" },
        "distance_over_time": { "$ref": "#/$defs/property_distance_over_time" },
        "electric_consumption_rate_since_start": { "$ref": "#/$defs/property_energy_efficiency" },
        "electric_consumption_rate_since_reset": { "$ref": "#/$defs/property_energy_efficiency" },
        "electric_distance_last_trip": { "$ref": "#/$defs/property_length" },
        "electric_distance_since_reset": { "$ref": "#/$defs/property_length" },
        "electric_duration_last_trip": { "$ref": "#/$defs/property_duration" },
        "electric_duration_since_reset": { "$ref": "#/$defs/property_duration" },
        "fuel_consumption_rate_last_trip": { "$ref": "#/$defs/property_fuel_efficiency" },
        "fuel_consumption_rate_since_reset": { "$ref": "#/$defs/property_fuel_efficiency" },
        "average_speed_last_trip": { "$ref": "#/$defs/property_speed" },
        "average_speed_since_reset": { "$ref": "#/$defs/property_speed" },
        "fuel_distance_last_trip": { "$ref": "#/$defs/property_length" },
        "fuel_distance_since_reset": { "$ref": "#/$defs/property_length" },
        "driving_duration_last_trip": { "$ref": "#/$defs/property_duration" },
        "driving_duration_since_reset": { "$ref": "#/$defs/property_duration" },
        "eco_score_total": { "$ref": "#/$defs/property_percentage" },
        "eco_score_free_wheel": { "$ref": "#/$defs/property_percentage" },
        "eco_score_constant": { "$ref": "#/$defs/property_percentage" },
        "eco_score_bonus_range": { "$ref": "#/$defs/property_length" },
        "trip_meters": { "$ref": "#/$defs/property_trip_meter" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property_distance_over_time": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/distance_over_time"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_driving_mode_activation_period": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/driving_mode_activation_period"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_driving_mode_energy_consumption": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/driving_mode_energy_consumption"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_duration": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/duration"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_energy": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/energy"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_energy_efficiency": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/energy_efficiency"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_fuel_efficiency": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/fuel_efficiency"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_grade": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/grade"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_length": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length"
                }
