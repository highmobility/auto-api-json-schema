{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/trips.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "type": { "$ref": "#/$defs/property_type" },
        "driver_name": { "$ref": "#/$defs/property_string" },
        "description": { "$ref": "#/$defs/property_string" },
        "start_time": { "$ref": "#/$defs/property_timestamp" },
        "end_time": { "$ref": "#/$defs/property_timestamp" },
        "start_address": { "$ref": "#/$defs/property_string" },
        "end_address": { "$ref": "#/$defs/property_string" },
        "start_coordinates": { "$ref": "#/$defs/property_coordinates" },
        "end_coordinates": { "$ref": "#/$defs/property_coordinates" },
        "start_odometer": { "$ref": "#/$defs/property_length" },
        "end_odometer": { "$ref": "#/$defs/property_length" },
        "average_fuel_consumption": { "$ref": "#/$defs/property_fuel_efficiency" },
        "distance": { "$ref": "#/$defs/property_length" },
        "start_address_components": { "$ref": "#/$defs/property_address_component" },
        "end_address_components": { "$ref": "#/$defs/property_address_component" },
        "event": { "$ref": "#/$defs/property_event" },
        "eco_level": { "$ref": "#/$defs/property_eco_level" },
        "thresholds": { "$ref": "#/$defs/property_eco_driving_threshold" },
        "total_fuel_consumption": { "$ref": "#/$defs/property_volume" },
        "total_idle_fuel_consumption": { "$ref": "#/$defs/property_volume" },
        "maximum_speed": { "$ref": "#/$defs/property_speed" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },


        "property_address_component": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/address_component"
                }
            }
        },

        "property_coordinates": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/coordinates"
                }
            }
        },

        "property_eco_driving_threshold": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/eco_driving_threshold"
                }
            }
        },

        "property_eco_level": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["high", "medium"]
                }
            }
        },

        "property_event": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["harsh_braking", "harsh_acceleration", "sharp_turn", "over_rpm", "overspeed", "idling_engine_on"]
                }
            }
        },

        "property_fuel_efficiency": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/fuel_efficiency"
                }
            }
        },

        "property_length": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length"
                }
            }
        },

        "property_nonce": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_speed": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/speed"
                }
            }
        },

        "property_string": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string"
                }
            }
        },

        "property_timestamp": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "format": "date-time"
                }
            }
        },

        "property_type": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["single", "multi", "eco"]
                }
            }
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        },

        "property_volume": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/volume"
                }
            }
        }
    }
}
