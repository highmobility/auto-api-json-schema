            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/position",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/position"
                }
            }
        },

        "property_power": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/power",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/power"
