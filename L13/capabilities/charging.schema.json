            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/active_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/active_state"
                }
            }
        },

