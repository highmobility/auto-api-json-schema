            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/status",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
