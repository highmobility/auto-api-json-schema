            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/starter_battery_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["red", "yellow", "green"]
                }
