            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/enabled_state"
                }
            }
        },

        "property_energy": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/energy",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/energy"
                }
            }
        },

        "property_length": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/length",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length"
                }
            }
        },

