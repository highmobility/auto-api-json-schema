{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "estimated_range": { "$ref": "#/$defs/property_length" },
        "battery_level": { "$ref": "#/$defs/property_percentage" },
        "battery_current_ac": {
            "$ref": "#/$defs/property_electric_current",
            "deprecated": true
        },
        "battery_current_dc": {
            "$ref": "#/$defs/property_electric_current",
            "deprecated": true
        },
        "charger_voltage_ac": {
            "$ref": "#/$defs/property_electric_potential_difference",
            "deprecated": true
        },
        "charger_voltage_dc": {
            "$ref": "#/$defs/property_electric_potential_difference",
            "deprecated": true
        },
        "charge_limit": { "$ref": "#/$defs/property_percentage" },
        "time_to_complete_charge": { "$ref": "#/$defs/property_duration" },
        "charging_rate_kw": {
            "$ref": "#/$defs/property_power",
            "deprecated": true
        },
        "charge_port_state": { "$ref": "#/$defs/property_position" },
        "charge_mode": { "$ref": "#/$defs/property_charge_mode" },
        "max_charging_current": { "$ref": "#/$defs/property_electric_current" },
        "plug_type": { "$ref": "#/$defs/property_plug_type" },
        "charging_window_chosen": { "$ref": "#/$defs/property_charging_window_chosen" },
        "departure_times": { "$ref": "#/$defs/property_departure_time" },
        "reduction_times": { "$ref": "#/$defs/property_reduction_time" },
        "battery_temperature": { "$ref": "#/$defs/property_temperature" },
        "timers": { "$ref": "#/$defs/property_timer" },
        "plugged_in": { "$ref": "#/$defs/property_plugged_in" },
        "status": { "$ref": "#/$defs/property_status" },
        "charging_rate": { "$ref": "#/$defs/property_power" },
        "battery_current": { "$ref": "#/$defs/property_electric_current" },
        "charger_voltage": { "$ref": "#/$defs/property_electric_potential_difference" },
        "current_type": { "$ref": "#/$defs/property_current_type" },
        "max_range": { "$ref": "#/$defs/property_length" },
        "starter_battery_state": { "$ref": "#/$defs/property_starter_battery_state" },
        "smart_charging_status": { "$ref": "#/$defs/property_smart_charging_status" },
        "battery_level_at_departure": { "$ref": "#/$defs/property_percentage" },
        "preconditioning_departure_status": { "$ref": "#/$defs/property_active_state" },
        "preconditioning_immediate_status": { "$ref": "#/$defs/property_active_state" },
        "preconditioning_departure_enabled": { "$ref": "#/$defs/property_enabled_state" },
        "preconditioning_error": { "$ref": "#/$defs/property_preconditioning_error" },
        "battery_capacity": { "$ref": "#/$defs/property_energy" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_active_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/active_state"
                }
            }
        },

        "property_charge_mode": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["immediate", "timer_based", "inductive"]
                }
            }
        },

        "property_charging_window_chosen": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["not_chosen", "chosen"]
                }
            }
        },

        "property_current_type": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["alternating_current", "direct_current"]
                }
            }
        },

        "property_departure_time": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/departure_time"
                }
            }
        },

        "property_duration": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/duration"
                }
            }
        },

        "property_electric_current": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/electric_current"
                }
            }
        },

        "property_electric_potential_difference": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/electric_potential_difference"
                }
            }
        },

        "property_enabled_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/enabled_state"
                }
            }
        },

        "property_energy": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/energy"
                }
            }
        },

        "property_length": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length"
                }
            }
        },

        "property_nonce": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_percentage": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/percentage"
                }
            }
        },

        "property_plug_type": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["type_1", "type_2", "ccs", "chademo"]
                }
            }
        },

        "property_plugged_in": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["disconnected", "plugged_in"]
                }
            }
        },

        "property_position": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/position"
                }
            }
        },

        "property_power": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/power"
                }
            }
        },

        "property_preconditioning_error": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["no_change", "not_possible_low", "not_possible_finished", "available_after_engine_restart", "general_error"]
                }
            }
        },

        "property_reduction_time": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/reduction_time"
                }
            }
        },

        "property_smart_charging_status": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["wallbox_is_active", "scc_is_active", "peak_setting_active"]
                }
            }
        },

        "property_starter_battery_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["red", "yellow", "green"]
                }
            }
        },

        "property_status": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["not_charging", "charging", "charging_complete", "initialising", "charging_paused", "charging_error", "cable_unplugged", "slow_charging", "fast_charging", "discharging", "foreign_object_detected"]
                }
            }
        },

        "property_temperature": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/temperature"
                }
            }
        },

        "property_timer": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/timer"
                }
            }
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
