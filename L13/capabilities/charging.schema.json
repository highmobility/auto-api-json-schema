            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/charge_mode",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["immediate", "timer_based", "inductive"]
                }
            }
        },

        "property_charging_window_chosen": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/charging_window_chosen",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["not_chosen", "chosen"]
                }
            }
        },

