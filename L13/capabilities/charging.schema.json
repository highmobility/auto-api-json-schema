                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_percentage": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property/percentage",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/charging.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/percentage"
                }
            }
        },

