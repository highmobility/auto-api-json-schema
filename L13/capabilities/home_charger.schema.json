{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "charging_status": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/charging_status" },
        "authentication_mechanism": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/authentication_mechanism" },
        "plug_type": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/plug_type" },
        "charging_power_kw": {
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/power",
            "deprecated": true
        },
        "solar_charging": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/active_state" },
        "wi_fi_hotspot_enabled": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/enabled_state" },
        "wi_fi_hotspot_ssid": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/string" },
        "wi_fi_hotspot_security": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/network_security" },
        "wi_fi_hotspot_password": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/string" },
        "authentication_state": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/authentication_state" },
        "charge_current": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/electric_current" },
        "maximum_charge_current": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/electric_current" },
        "minimum_charge_current": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/electric_current" },
        "coordinates": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/coordinates" },
        "price_tariffs": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/price_tariff" },
        "charging_power": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/power" },

        "nonce": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/nonce" },
        "vin": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/failure"
                },
                "availability": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/availability"
                }
            },
            "minProperties": 1
        },


        "property_active_state": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/active_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/active_state"
                }
            }
        },

        "property_authentication_mechanism": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/authentication_mechanism",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["pin", "app"]
                }
            }
        },

        "property_authentication_state": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/authentication_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["unauthenticated", "authenticated"]
                }
            }
        },

        "property_charging_status": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/charging_status",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["disconnected", "plugged_in", "charging"]
                }
            }
        },

        "property_coordinates": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/coordinates",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/coordinates"
                }
            }
        },

        "property_electric_current": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/electric_current",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/electric_current"
                }
            }
        },

        "property_enabled_state": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/enabled_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/enabled_state"
                }
            }
        },

        "property_network_security": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/network_security",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/network_security"
                }
            }
        },

        "property_nonce": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/nonce",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/bytes",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_plug_type": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/plug_type",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["type_1", "type_2", "ccs", "chademo"]
                }
            }
        },

        "property_power": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/power",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/power"
                }
            }
        },

        "property_price_tariff": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/price_tariff",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/price_tariff"
                }
            }
        },

        "property_string": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/string",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "type": "string"
                }
            }
        },

        "property_vin": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property/vin",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
