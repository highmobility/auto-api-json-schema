{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/home_charger.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "charging_status": { "$ref": "#/$defs/property_charging_status" },
        "authentication_mechanism": { "$ref": "#/$defs/property_authentication_mechanism" },
        "plug_type": { "$ref": "#/$defs/property_plug_type" },
        "charging_power_kw": {
            "$ref": "#/$defs/property_power",
            "deprecated": true
        },
        "solar_charging": { "$ref": "#/$defs/property_active_state" },
        "wi_fi_hotspot_enabled": { "$ref": "#/$defs/property_enabled_state" },
        "wi_fi_hotspot_ssid": { "$ref": "#/$defs/property_string" },
        "wi_fi_hotspot_security": { "$ref": "#/$defs/property_network_security" },
        "wi_fi_hotspot_password": { "$ref": "#/$defs/property_string" },
        "authentication_state": { "$ref": "#/$defs/property_authentication_state" },
        "charge_current": { "$ref": "#/$defs/property_electric_current" },
        "maximum_charge_current": { "$ref": "#/$defs/property_electric_current" },
        "minimum_charge_current": { "$ref": "#/$defs/property_electric_current" },
        "coordinates": { "$ref": "#/$defs/property_coordinates" },
        "price_tariffs": { "$ref": "#/$defs/property_price_tariff" },
        "charging_power": { "$ref": "#/$defs/property_power" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_active_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/active_state"
                }
            }
        },

        "property_authentication_mechanism": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["pin", "app"]
                }
            }
        },

        "property_authentication_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["unauthenticated", "authenticated"]
                }
            }
        },

        "property_charging_status": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["disconnected", "plugged_in", "charging"]
                }
            }
        },

        "property_coordinates": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/coordinates"
                }
            }
        },

        "property_electric_current": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/electric_current"
                }
            }
        },

        "property_enabled_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/enabled_state"
                }
            }
        },

        "property_network_security": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/network_security"
                }
            }
        },

        "property_nonce": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_plug_type": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["type_1", "type_2", "ccs", "chademo"]
                }
            }
        },

        "property_power": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/power"
                }
            }
        },

        "property_price_tariff": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/price_tariff"
                }
            }
        },

        "property_string": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string"
                }
            }
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
