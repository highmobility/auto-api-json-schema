{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/adas.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "status": { "$ref": "#/$defs/property_on_off_state" },
        "alertness_system_status": { "$ref": "#/$defs/property_active_state" },
        "forward_collision_warning_system": { "$ref": "#/$defs/property_active_state" },
        "blind_spot_warning_state": { "$ref": "#/$defs/property_active_state" },
        "blind_spot_warning_system_coverage": { "$ref": "#/$defs/property_blind_spot_warning_system_coverage" },
        "rear_cross_warning_system": { "$ref": "#/$defs/property_active_state" },
        "automated_parking_brake": { "$ref": "#/$defs/property_active_state" },
        "lane_keep_assist_system": { "$ref": "#/$defs/property_on_off_state" },
        "lane_keep_assists_states": { "$ref": "#/$defs/property_lane_keep_assist_state" },
        "park_assists": { "$ref": "#/$defs/property_park_assist" },
        "blind_spot_warning_system": { "$ref": "#/$defs/property_on_off_state" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },


        "property_active_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/active_state"
                }
            }
        },

        "property_blind_spot_warning_system_coverage": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["regular", "trailer"]
                }
            }
        },

        "property_lane_keep_assist_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/lane_keep_assist_state"
                }
            }
        },

        "property_nonce": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_on_off_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/on_off_state"
                }
            }
        },

        "property_park_assist": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/park_assist"
                }
            }
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
