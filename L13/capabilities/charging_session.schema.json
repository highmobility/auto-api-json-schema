{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/capabilities/charging_session.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "public_charging_points": { "$ref": "#/$defs/property_charging_point" },
        "displayed_state_of_charge": { "$ref": "#/$defs/property_percentage" },
        "displayed_start_state_of_charge": { "$ref": "#/$defs/property_percentage" },
        "business_errors": { "$ref": "#/$defs/property_string" },
        "time_zone": { "$ref": "#/$defs/property_string" },
        "start_time": { "$ref": "#/$defs/property_timestamp" },
        "end_time": { "$ref": "#/$defs/property_timestamp" },
        "total_charging_duration": { "$ref": "#/$defs/property_duration" },
        "calculated_energy_charged": { "$ref": "#/$defs/property_energy" },
        "energy_charged": { "$ref": "#/$defs/property_energy" },
        "preconditioning_state": { "$ref": "#/$defs/property_active_state" },
        "odometer": { "$ref": "#/$defs/property_length" },
        "charging_cost": { "$ref": "#/$defs/property_charging_cost" },
        "location": { "$ref": "#/$defs/property_charging_location" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_active_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/active_state"
                }
            }
        },

        "property_charging_cost": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/charging_cost"
                }
            }
        },

        "property_charging_location": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/charging_location"
                }
            }
        },

        "property_charging_point": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/charging_point"
                }
            }
        },

        "property_duration": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/duration"
                }
            }
        },

        "property_energy": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/energy"
                }
            }
        },

        "property_length": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length"
                }
            }
        },

        "property_nonce": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_percentage": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/percentage"
                }
            }
        },

        "property_string": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string"
                }
            }
        },

        "property_timestamp": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "format": "date-time"
                }
            }
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
