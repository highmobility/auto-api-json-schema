{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/tachograph.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "drivers_working_states": { "$ref": "#/$defs/property_driver_working_state" },
        "drivers_time_states": { "$ref": "#/$defs/property_driver_time_state" },
        "drivers_cards_present": { "$ref": "#/$defs/property_driver_card_present" },
        "vehicle_motion": { "$ref": "#/$defs/property_detected" },
        "vehicle_overspeed": { "$ref": "#/$defs/property_vehicle_overspeed" },
        "vehicle_direction": { "$ref": "#/$defs/property_vehicle_direction" },
        "vehicle_speed": { "$ref": "#/$defs/property_speed" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },

        "property_detected": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/detected"
                }
            }
        },

        "property_driver_card_present": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/driver_card_present"
                }
            }
        },

        "property_driver_time_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/driver_time_state"
                }
            }
        },

        "property_driver_working_state": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/driver_working_state"
                }
            }
        },

        "property_nonce": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_speed": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/speed"
                }
            }
        },

        "property_vehicle_direction": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["forward", "reverse"]
                }
            }
        },

        "property_vehicle_overspeed": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["no_overspeed", "overspeed"]
                }
            }
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
