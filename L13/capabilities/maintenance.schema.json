{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/capabilities/maintenance.schema.json",
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "days_to_next_service": {
            "$ref": "#/$defs/property_duration",
            "deprecated": true
        },
        "kilometers_to_next_service": {
            "$ref": "#/$defs/property_length",
            "deprecated": true
        },
        "cbs_reports_count": { "$ref": "#/$defs/property_uinteger" },
        "months_to_exhaust_inspection": {
            "$ref": "#/$defs/property_duration",
            "deprecated": true
        },
        "teleservice_availability": { "$ref": "#/$defs/property_teleservice_availability" },
        "service_distance_threshold": { "$ref": "#/$defs/property_length" },
        "service_time_threshold": { "$ref": "#/$defs/property_duration" },
        "automatic_teleservice_call_date": { "$ref": "#/$defs/property_timestamp" },
        "teleservice_battery_call_date": { "$ref": "#/$defs/property_timestamp" },
        "next_inspection_date": { "$ref": "#/$defs/property_timestamp" },
        "condition_based_services": { "$ref": "#/$defs/property_condition_based_service" },
        "brake_fluid_change_date": { "$ref": "#/$defs/property_timestamp" },
        "time_to_next_service": { "$ref": "#/$defs/property_duration" },
        "distance_to_next_service": { "$ref": "#/$defs/property_length" },
        "time_to_exhaust_inspection": { "$ref": "#/$defs/property_duration" },
        "last_ecall": { "$ref": "#/$defs/property_timestamp" },
        "distance_to_next_oil_service": { "$ref": "#/$defs/property_length" },
        "time_to_next_oil_service": { "$ref": "#/$defs/property_duration" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property_condition_based_service": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/condition_based_service"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_duration": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/duration"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_length": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_nonce": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_teleservice_availability": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["pending", "idle", "successful", "error"]
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_timestamp": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "format": "date-time"
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_uinteger": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                }
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
