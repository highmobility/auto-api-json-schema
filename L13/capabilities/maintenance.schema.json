{
    "$id": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/all-properties-multiple/L13/capabilities/maintenance.schema.json",
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "days_to_next_service": {
            "description": "Time until next servicing of the car",
        	"deprecated": true,
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_duration"
            }
        },
        "kilometers_to_next_service": {
            "description": "The distance until next servicing of the vehicle",
        	"deprecated": true,
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "cbs_reports_count": {
            "description": "The number of CBS reports",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_uinteger"
            }
        },
        "months_to_exhaust_inspection": {
            "description": "Time until exhaust inspection",
        	"deprecated": true,
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_duration"
            }
        },
        "teleservice_availability": {
            "description": "Teleservice availability",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_teleservice_availability"
            }
        },
        "service_distance_threshold": {
            "description": "Distance threshold for service",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "service_time_threshold": {
            "description": "Time threshold for service",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_duration"
            }
        },
        "automatic_teleservice_call_date": {
            "description": "Automatic teleservice call date",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "teleservice_battery_call_date": {
            "description": "Teleservice batter call date",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "next_inspection_date": {
            "description": "Next inspection date",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "condition_based_services": {
            "description": "Condition based services",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_condition_based_service"
            }
        },
        "brake_fluid_change_date": {
            "description": "Brake fluid change date",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "time_to_next_service": {
            "description": "Time until next servicing of the vehicle",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_duration"
            }
        },
        "distance_to_next_service": {
            "description": "The distance until next servicing of the vehicle",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "time_to_exhaust_inspection": {
            "description": "Time until exhaust inspection",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_duration"
            }
        },
        "last_ecall": {
            "description": "Date-time of the last eCall",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "distance_to_next_oil_service": {
            "description": "Indicates the remaining distance until the next oil service; if this limit was exceeded, this value indicates the distance that has been driven since then.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "time_to_next_oil_service": {
            "description": "Indicates the time remaining until the next oil service; if this limit was exceeded, this value indicates the time that has passed since then.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_duration"
            }
        },
        "brake_fluid_remaining_distance": {
            "description": "Indicates the remaining distance for brake fluid.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "brake_fluid_status": {
            "description": "Brake fluid's service status.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_service_status"
            }
        },
        "brakes_service_due_dates": {
            "description": "Brakes servicing due dates.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_brake_service_due_date"
            }
        },
        "brakes_service_remaining_distances": {
            "description": "Brakes servicing remaining distances.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_brake_service_remaining_distance"
            }
        },
        "brakes_service_statuses": {
            "description": "Brakes servicing statuses.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_brake_service_status"
            }
        },
        "drive_in_inspection_date": {
            "description": "Next drive-in inspection date.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "drive_in_inspection_status": {
            "description": "Drive-in inspection service status.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_service_status"
            }
        },
        "next_oil_service_date": {
            "description": "Next oil service date.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "next_inspection_distance_to": {
            "description": "Distance until the next inspection.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "legal_inspection_date": {
            "description": "Next legally required inspection date",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "service_status": {
            "description": "Consolidated status regarding service requirements. OK: no current service requirement, WARNING: at least one service has reported requirement, CRITICAL: at least one service is overdue.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_service_status"
            }
        },
        "service_date": {
            "description": "Date of the earliest service. If this service is overdue, the date is in the past.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "inspection_status": {
            "description": "Vehicle inspection service status.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_service_status"
            }
        },
        "drive_in_inspection_distance_to": {
            "description": "The distance until next drive-in inspection of the vehicle",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "vehicle_check_date": {
            "description": "Vehicle check date (usually after a predetermined distance).",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "vehicle_check_status": {
            "description": "Vehicle check service status.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_service_status"
            }
        },
        "vehicle_check_distance_to": {
            "description": "The distance until next vehicle check.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "service_warning": {
            "description": "Represents the type of service required and how soon it must be performed.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_service_warning"
            }
        },
        "last_service_date": {
            "description": "Date of the last service performed on the vehicle.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_timestamp"
            }
        },
        "last_service_distance": {
            "description": "Distance reading at the time of the last service.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "next_inspection_distance": {
            "description": "Distance reading at which the next inspection is due.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property_brake_service_due_date": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/brake_service_due_date"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_brake_service_remaining_distance": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/brake_service_remaining_distance"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_brake_service_status": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/brake_service_status"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_condition_based_service": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/condition_based_service"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_duration": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/units.schema.json#/$defs/duration"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_length": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/units.schema.json#/$defs/length"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_nonce": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_service_status": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/service_status"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_service_warning": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/service_warning"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_teleservice_availability": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["pending", "idle", "successful", "error"]
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_timestamp": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "format": "date-time"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_uinteger": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_vin": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        }
    }
}
