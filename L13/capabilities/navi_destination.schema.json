{
    "$id": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/all-properties-multiple/L13/capabilities/navi_destination.schema.json",
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "coordinates": {
            "description": "Coordinates",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_coordinates"
            }
        },
        "destination_name": {
            "description": "Destination name",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_string"
            }
        },
        "data_slots_free": {
            "description": "Remaining number of POI data slots available.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_uinteger"
            }
        },
        "data_slots_max": {
            "description": "Maximum number of POI data slots.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_uinteger"
            }
        },
        "arrival_duration": {
            "description": "Remaining time until reaching the destination.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_duration"
            }
        },
        "distance_to_destination": {
            "description": "Remaining distance to reach the destination.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_length"
            }
        },
        "battery_consumption_to_destination": {
            "description": "Required battery charge level to proceed to the next destination.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_percentage"
            }
        },
        "charging_time_for_destination": {
            "description": "Required charging time to reach the next destination.",
            "type": "array",
            "items": {
                "$ref": "#/$defs/property_duration"
            }
        },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,


    "$defs": {
        "property_coordinates": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/coordinates"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_duration": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/units.schema.json#/$defs/duration"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_length": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/units.schema.json#/$defs/length"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_nonce": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_percentage": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/percentage"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_string": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_uinteger": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_vin": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/highmobility/auto-api-json-schema/all-properties-multiple/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1,
            "additionalProperties": false
        }
    }
}
