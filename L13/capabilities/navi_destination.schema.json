        "property_coordinates": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/coordinates"
                },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
        "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }.indented(byLevel: 2))
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_duration": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/duration"
                },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
        "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }.indented(byLevel: 2))
            },
            "minProperties": 1,
            "additionalProperties": false
        },

        "property_length": {
            "type": "object",
            "properties": {
