{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "status": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/enabled_state" },
        "network_connected": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/connection_state" },
        "network_ssid": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/string" },
        "network_security": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/network_security" },

        "nonce": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/nonce" },
        "vin": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/failure"
                },
                "availability": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/availability"
                }
            },
            "minProperties": 1
        },


        "property_connection_state": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/connection_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/connection_state"
                }
            }
        },

        "property_enabled_state": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/enabled_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/enabled_state"
                }
            }
        },

        "property_network_security": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/network_security",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/network_security"
                }
            }
        },

        "property_nonce": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/nonce",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/bytes",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_string": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/string",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property",
            "properties": {
                "data": {
                    "type": "string"
                }
            }
        },

        "property_vin": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property/vin",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/wi_fi.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
