{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "front_exterior_light": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/front_exterior_light" },
        "rear_exterior_light": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/active_state" },
        "ambient_light_colour": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/rgb_colour" },
        "reverse_light": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/active_state" },
        "emergency_brake_light": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/active_state" },
        "fog_lights": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/light" },
        "reading_lamps": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/reading_lamp" },
        "interior_lights": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/light" },
        "switch_position": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/switch_position" },
        "parking_light_status": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/parking_light_status" },

        "nonce": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/nonce" },
        "vin": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/failure"
                },
                "availability": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json/availability"
                }
            },
            "minProperties": 1
        },


        "property_active_state": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/active_state",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/active_state"
                }
            }
        },

        "property_front_exterior_light": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/front_exterior_light",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["inactive", "active", "active_with_full_beam", "drl", "automatic"]
                }
            }
        },

        "property_light": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/light",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/light"
                }
            }
        },

        "property_nonce": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/nonce",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/bytes",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_parking_light_status": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/parking_light_status",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["off", "left", "right", "both"]
                }
            }
        },

        "property_reading_lamp": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/reading_lamp",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/reading_lamp"
                }
            }
        },

        "property_rgb_colour": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/rgb_colour",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "properties": {
                "data": {
                    "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/rgb_colour"
                }
            }
        },

        "property_switch_position": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/switch_position",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["automatic", "dipped_headlights", "parking_light_right", "parking_light_left", "sidelights"]
                }
            }
        },

        "property_vin": {
            "$id": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property/vin",
            "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/lights.schema.json/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
