{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/chassis_settings.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "driving_mode": { "$ref": "#/$defs/property_driving_mode" },
        "sport_chrono": { "$ref": "#/$defs/property_sport_chrono" },
        "current_spring_rates": { "$ref": "#/$defs/property_spring_rate" },
        "maximum_spring_rates": { "$ref": "#/$defs/property_spring_rate" },
        "minimum_spring_rates": { "$ref": "#/$defs/property_spring_rate" },
        "current_chassis_position": { "$ref": "#/$defs/property_length" },
        "maximum_chassis_position": { "$ref": "#/$defs/property_length" },
        "minimum_chassis_position": { "$ref": "#/$defs/property_length" },

        "nonce": { "$ref": "#/$defs/property_nonce" },
        "vin": { "$ref": "#/$defs/property_vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },


        "property_driving_mode": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/driving_mode"
                }
            }
        },

        "property_length": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length"
                }
            }
        },

        "property_nonce": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_sport_chrono": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["inactive", "active", "reset"]
                }
            }
        },

        "property_spring_rate": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "$ref": "/auto-api-json-schema/L13/misc/custom_types.schema.json#/$defs/spring_rate"
                }
            }
        },

        "property_vin": {
            "$ref": "#/$defs/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
