{
    "$id": "https://highmobility.github.io/auto-api-json-schema/L13/misc/custom_types.schema.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "description": "AutoAPI custom types",

    "$defs": {
        "active_state": {
            "type": "string",
            "enum": ["inactive", "active"]
        },

        "active_selected_state": {
            "type": "string",
            "enum": ["inactive_selected", "inactive_not_selected", "active"]
        },

        "axle": {
            "type": "string",
            "enum": ["front", "rear"]
        },

        "connection_state": {
            "type": "string",
            "enum": ["disconnected", "connected"]
        },

        "coordinates": {
            "type": "object",
            "properties": {
                "latitude": { "type": "number" },
                "longitude": { "type": "number" }
            }
        },

        "detected": {
            "type": "string",
            "enum": ["not_detected", "detected"]
        },

        "driving_mode": {
            "type": "string",
            "enum": ["regular", "eco", "sport", "sport_plus", "ecoPlus", "comfort"]
        },

        "enabled_state": {
            "type": "string",
            "enum": ["disabled", "enabled"]
        },

        "engine_type": {
            "type": "string",
            "enum": ["unknown", "all_electric", "combustion_engine", "phev", "hydrogen", "hydrogen_hybrid", "petrol", "electric", "gas", "diesel", "gasoline", "cng", "lpg"]
        },

        "fluid_level": {
            "type": "string",
            "enum": ["low", "filled", "very_low", "normal", "high", "very_high"]
        },

        "grade": {
            "type": "string",
            "enum": ["excellent", "normal", "warning"]
        },

        "ignition_state": {
            "type": "string",
            "enum": ["lock", "off", "accessory", "on", "start"]
        },

        "key_value": {
            "type": "object",
            "properties": {
                "key": { "type": "string" },
                "value": { "type": "string" }
            }
        },

        "location": {
            "type": "string",
            "enum": ["front_left", "front_right", "rear_right", "rear_left"]
        },

        "location_longitudinal": {
            "type": "string",
            "enum": ["front", "rear"]
        },

        "location_wheel": {
            "type": "string",
            "enum": ["front_left", "front_right", "rear_right", "rear_left", "rear_right_outer", "rear_left_outer", "spare"]
        },

        "lock_state": {
            "type": "string",
            "enum": ["unlocked", "locked"]
        },

        "lock_safety": {
            "type": "string",
            "enum": ["safe", "unsafe"]
        },

        "muted": {
            "type": "string",
            "enum": ["not_muted", "muted"]
        },

        "network_security": {
            "type": "string",
            "enum": ["none", "wep", "wpa", "wpa2_personal"]
        },

        "on_off_state": {
            "type": "string",
            "enum": ["off", "on"]
        },

        "percentage": {
            "type": "number"
        },

        "position": {
            "type": "string",
            "enum": ["closed", "open"]
        },

        "seat_location": {
            "type": "string",
            "enum": ["front_left", "front_right", "rear_right", "rear_left", "rear_center"]
        },

        "start_stop": {
            "type": "string",
            "enum": ["start", "stop"]
        },

        "supported_capability": {
            "type": "object",
            "properties": {
                "capability_id": { "type": "integer" },
                "supported_property_ids": {
                  "type": "array",
                  "items": { "type": "integer" }
                }
            }
        },

        "time": {
            "type": "object",
            "properties": {
                "hour": { "type": "integer" },
                "minute": { "type": "integer" }
            }
        },

        "weekday": {
            "type": "string",
            "enum": ["monday", "tuesday", "wednesday", "thursday", "friday", "saturday", "sunday", "automatic"]
        },

        "window_location": {
            "type": "string",
            "enum": ["front_left", "front_right", "rear_right", "rear_left", "hatch"]
        },

        "zone": {
            "type": "object",
            "properties": {
                "horizontal": { "type": "integer" },
                "vertical": { "type": "integer" }
            }
        },

        "acceleration": {
            "type": "object",
            "properties": {
                "direction": {
                    "type": "string",
                    "enum": ["longitudinal", "lateral", "front_lateral", "rear_lateral"]
                },
                "acceleration": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/acceleration" }
            }
        },

        "action_item": {
            "type": "object",
            "properties": {
                "id": { "type": "integer" },
                "name": { "type": "string" }
            }
        },

        "address_component": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "enum": ["city", "country", "country_short", "district", "postal_code", "street", "state_province", "other"]
                },
                "value": { "type": "string" }
            }
        },

        "availability": {
            "type": "object",
            "properties": {
                "update_rate": {
                    "type": "string",
                    "enum": ["trip_high", "trip", "trip_start_end", "trip_end", "unknown", "not_available", "on_change"]
                },
                "rate_limit": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/frequency" },
                "applies_per": {
                    "type": "string",
                    "enum": ["app", "vehicle"]
                }
            }
        },

        "brake_torque_vectoring": {
            "type": "object",
            "properties": {
                "axle": { "$ref": "#/$defs/axle" },
                "state": { "$ref": "#/$defs/state" }
            }
        },

        "charging_cost": {
            "type": "object",
            "properties": {
                "currency": { "type": "string" },
                "calculated_charging_cost": { "type": "number" },
                "calculated_savings": { "type": "number" },
                "simulated_immediate_charging_cost": { "type": "number" }
            }
        },

        "charging_location": {
            "type": "object",
            "properties": {
                "municipality": { "type": "string" },
                "formatted_address": { "type": "string" },
                "street_address": { "type": "string" }
            }
        },

        "charging_point": {
            "type": "object",
            "properties": {
                "city": { "type": "string" },
                "postal_code": { "type": "string" },
                "street": { "type": "string" },
                "provider": { "type": "string" }
            }
        },

        "check_control_message": {
            "type": "object",
            "properties": {
                "id": { "type": "integer" },
                "remaining_time": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/duration" },
                "text": { "type": "string" },
                "status": { "type": "string" }
            }
        },

        "condition_based_service": {
            "type": "object",
            "properties": {
                "year": { "type": "integer" },
                "month": { "type": "integer" },
                "id": { "type": "integer" },
                "due_status": {
                    "type": "string",
                    "enum": ["ok", "pending", "overdue"]
                },
                "text": { "type": "string" },
                "description": { "type": "string" }
            }
        },

        "confirmed_trouble_code": {
            "type": "object",
            "properties": {
                "id": { "type": "string" },
                "ecu_address": { "type": "string" },
                "ecu_variant_name": { "type": "string" },
                "status": { "type": "string" }
            }
        },

        "crash_incident": {
            "type": "object",
            "properties": {
                "location": {
                    "type": "string",
                    "enum": ["front", "lateral", "rear"]
                },
                "severity": {
                    "type": "string",
                    "enum": ["very_high", "high", "medium", "low"]
                },
                "repairs": {
                    "type": "string",
                    "enum": ["unknown", "needed", "not_needed"]
                }
            }
        },

        "dashboard_light": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "enum": ["high_beam", "low_beam", "hazard_warning", "brake_failure", "hatch_open", "fuel_level", "engine_coolant_temperature", "battery_charging_condition", "engine_oil", "position_lights", "front_fog_light", "rear_fog_light", "park_heating", "engine_indicator", "service_call", "transmission_fluid_temperature", "transmission_failure", "anti_lock_brake_failure", "worn_brake_linings", "windscreen_washer_fluid", "tire_failure", "engine_oil_level", "engine_coolant_level", "steering_failure", "esc_indication", "brake_lights", "adblue_level", "fuel_filter_diff_pressure", "seat_belt", "advanced_braking", "acc", "trailer_connected", "airbag", "esc_switched_off", "lane_departure_warning_off", "air_filter_minder", "air_suspension_ride_control_fault", "all_wheel_drive_disabled", "anti_theft", "blind_spot_detection", "charge_system_fault", "check_fuel_cap", "check_fuel_fill_inlet", "check_fuel_filter", "dc_temp_warning", "dc_warning_status", "diesel_engine_idle_shutdown", "diesel_engine_warning", "diesel_exhaust_fluid_system_fault", "diesel_exhaust_over_temp", "diesel_exhaust_fluid_quality", "diesel_filter_regeneration", "diesel_particulate_filter", "diesel_pre_heat", "electric_trailer_brake_connection", "ev_battery_cell_max_volt_warning", "ev_battery_cell_min_volt_warning", "ev_battery_charge_energy_storage_warning", "ev_battery_high_level_warning", "ev_battery_high_temperature_warning", "ev_battery_insulation_resist_warning", "ev_battery_jump_level_warning", "ev_battery_low_level_warning", "ev_battery_max_volt_veh_energy_warning", "ev_battery_min_volt_veh_energy_warning", "ev_battery_over_charge_warning", "ev_battery_poor_cell_warning", "ev_battery_temp_diff_warning", "forward_collision_warning", "fuel_door_open", "hill_descent_control_fault", "hill_start_assist_warning", "hv_interlocking_status_warning", "lighting_system_failure", "malfunction_indicator", "motor_controller_temp_warning", "park_aid_malfunction", "passive_entry_passive_start", "powertrain_malfunction", "restraints_indicator_warning", "start_stop_engine_warning", "traction_control_disabled", "traction_control_active", "traction_motor_temp_warning", "tire_pressure_monitor_system_warning", "water_in_fuel", "tire_warning_front_right", "tire_warning_front_left", "tire_warning_rear_right", "tire_warning_rear_left", "tire_warning_system_error", "battery_low_warning", "brake_fluid_warning", "active_hood_fault", "active_spoiler_fault", "adjust_tire_pressure", "steering_lock_alert", "anti_pollution_failure_engine_start_impossible", "anti_pollution_system_failure", "anti_reverse_system_failing", "auto_parking_brake", "automatic_braking_deactive", "automatic_braking_system_fault", "automatic_lights_settings_failure", "keyfob_battery_alarm", "trunk_open", "check_reversing_lamp", "crossing_line_system_alert_failure", "dipped_beam_headlamps_front_left_failure", "dipped_beam_headlamps_front_right_failure", "directional_headlamps_failure", "directional_light_failure", "dsg_failing", "electric_mode_not_available", "electronic_lock_failure", "engine_control_system_failure", "engine_oil_pressure_alert", "esp_failure", "excessive_oil_temperature", "tire_front_left_flat", "tire_front_right_flat", "tire_rear_left_flat", "tire_rear_right_flat", "fog_light_front_left_failure", "fog_light_front_right_failure", "fog_light_rear_left_failure", "fog_light_rear_right_failure", "fog_light_front_fault", "door_front_left_open", "door_front_left_open_high_speed", "tire_front_left_not_monitored", "door_front_right_open", "door_front_right_open_high_speed", "tire_front_right_not_monitored", "headlights_left_failure", "headlights_right_failure", "hybrid_system_fault", "hybrid_system_fault_repaired_vehicle", "hydraulic_pressure_or_brake_fuild_insufficient", "lane_departure_fault", "limited_visibility_aids_camera", "tire_pressure_low", "maintenance_date_exceeded", "maintenance_odometer_exceeded", "other_failing_system", "parking_brake_control_failing", "parking_space_measuring_system_failure", "place_gear_to_parking", "power_steering_assitance_failure", "power_steering_failure", "preheating_deactivated_battery_too_low", "preheating_deactivated_fuel_level_too_low", "preheating_deactivated_battery_set_the_clock", "fog_light_rear_fault", "door_rear_left_open", "door_rear_left_open_high_speed", "tire_rear_left_not_monitored", "door_rear_right_open", "door_rear_right_open_high_speed", "tire_rear_right_not_monitored", "screen_rear_open", "retractable_roof_mechanism_fault", "reverse_light_left_failure", "reverse_light_right_failure", "risk_of_ice", "roof_operation_impossible_apply_parking_break", "roof_operation_impossible_apply_start_engine", "roof_operation_impossible_temperature_too_high", "seatbelt_passenger_front_right_unbuckled", "seatbelt_passenger_rear_left_unbuckled", "seatbelt_passenger_rear_center_unbuckled", "seatbelt_passenger_rear_right_unbuckled", "battery_secondary_low", "shock_sensor_failing", "side_lights_front_left_failure", "side_lights_front_right_failure", "side_lights_rear_left_failure", "side_lights_rear_right_failure", "spare_wheel_fitter_driving_aids_deactivated", "speed_control_failure", "stop_light_left_failure", "stop_light_right_failure", "suspension_failure", "suspension_failure_reduce_speed", "suspension_fault_limited_to_90kmh", "tire_pressure_sensor_failure", "trunk_open_high_speed", "trunk_window_open", "turn_signal_front_left_failure", "turn_signal_front_right_failure", "turn_signal_rear_left_failure", "turn_signal_rear_right_failure", "tire_under_inflation", "wheel_pressure_fault", "oil_change_warning", "inspection_warning"]
                },
                "state": { "$ref": "#/$defs/state" }
            }
        },

        "departure_time": {
            "type": "object",
            "properties": {
                "state": { "$ref": "#/$defs/state" },
                "time": {
                    "type": "object",
                    "$ref": "#/$defs/time"
                }
            }
        },

        "diesel_exhaust_filter_status": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "string",
                    "enum": ["unknown", "normal_operation", "overloaded", "at_limit", "over_limit"]
                },
                "component": {
                    "type": "string",
                    "enum": ["unknown", "exhaust_filter", "diesel_particulate_filter", "overboost_code_regulator", "off_board_regeneration"]
                },
                "cleaning": {
                    "type": "string",
                    "enum": ["unknown", "in_progress", "complete", "interrupted"]
                }
            }
        },

        "distance_over_time": {
            "type": "object",
            "properties": {
                "distance": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length" },
                "time": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/duration" }
            }
        },

        "door_position": {
            "type": "object",
            "properties": {
                "location": {
                    "type": "string",
                    "enum": ["front_left", "front_right", "rear_right", "rear_left", "all"]
                },
                "position": { "$ref": "#/$defs/position" }
            }
        },

        "driver_card_present": {
            "type": "object",
            "properties": {
                "driver_number": { "type": "integer" },
                "card_present": {
                    "type": "string",
                    "enum": ["not_present", "present"]
                }
            }
        },

        "driver_time_state": {
            "type": "object",
            "properties": {
                "driver_number": { "type": "integer" },
                "time_state": {
                    "type": "string",
                    "enum": ["normal", "fifteen_min_before_four", "four_reached", "fifteen_min_before_nine", "nine_reached", "fifteen_min_before_sixteen", "sixteen_reached"]
                }
            }
        },

        "driver_working_state": {
            "type": "object",
            "properties": {
                "driver_number": { "type": "integer" },
                "working_state": {
                    "type": "string",
                    "enum": ["resting", "driver_available", "working", "driving"]
                }
            }
        },

        "driving_mode_activation_period": {
            "type": "object",
            "properties": {
                "driving_mode": { "$ref": "#/$defs/driving_mode" },
                "period": { "type": "number" }
            }
        },

        "driving_mode_energy_consumption": {
            "type": "object",
            "properties": {
                "driving_mode": { "$ref": "#/$defs/driving_mode" },
                "consumption": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/energy" }
            }
        },

        "eco_driving_threshold": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "enum": ["zero", "one"]
                },
                "value": { "type": "number" }
            }
        },

        "failure": {
            "type": "object",
            "properties": {
                "reason": {
                    "type": "string",
                    "enum": ["rate_limit", "execution_timeout", "format_error", "unauthorised", "unknown", "pending", "oem_error", "privacy_mode_active"]
                },
                "description": { "type": "string" }
            }
        },

        "hmkit_version": {
            "type": "object",
            "properties": {
                "major": { "type": "integer" },
                "minor": { "type": "integer" },
                "patch": { "type": "integer" }
            }
        },

        "hvac_weekday_starting_time": {
            "type": "object",
            "properties": {
                "weekday": { "$ref": "#/$defs/weekday" },
                "time": {
                    "type": "object",
                    "$ref": "#/$defs/time"
                }
            }
        },

        "lane_keep_assist_state": {
            "type": "object",
            "properties": {
                "location": {
                    "type": "string",
                    "enum": ["left", "right"]
                },
                "state": { "$ref": "#/$defs/state" }
            }
        },

        "light": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "state": { "$ref": "#/$defs/state" }
            }
        },

        "lock": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "lock_state": { "$ref": "#/$defs/lock_state" }
            }
        },

        "oem_trouble_code_value": {
            "type": "object",
            "properties": {
                "id": { "type": "string" },
                "key_value": {
                    "type": "object",
                    "$ref": "#/$defs/key_value"
                }
            }
        },

        "park_assist": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "alarm": { "$ref": "#/$defs/alarm" },
                "muted": { "$ref": "#/$defs/muted" }
            }
        },

        "person_detected": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "detected": { "$ref": "#/$defs/detected" }
            }
        },

        "price_tariff": {
            "type": "object",
            "properties": {
                "pricing_type": {
                    "type": "string",
                    "enum": ["starting_fee", "per_minute", "per_kwh"]
                },
                "price": { "type": "number" },
                "currency": { "type": "string" }
            }
        },

        "reading_lamp": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "state": { "$ref": "#/$defs/state" }
            }
        },

        "reduction_time": {
            "type": "object",
            "properties": {
                "start_stop": { "$ref": "#/$defs/start_stop" },
                "time": {
                    "type": "object",
                    "$ref": "#/$defs/time"
                }
            }
        },

        "rgb_colour": {
            "type": "object",
            "properties": {
                "red": { "type": "integer" },
                "green": { "type": "integer" },
                "blue": { "type": "integer" }
            }
        },

        "seatbelt_state": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "fastened_state": {
                    "type": "string",
                    "enum": ["not_fastened", "fastened"]
                }
            }
        },

        "spring_rate": {
            "type": "object",
            "properties": {
                "axle": { "$ref": "#/$defs/axle" },
                "spring_rate": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/torque" }
            }
        },

        "timer": {
            "type": "object",
            "properties": {
                "timer_type": {
                    "type": "string",
                    "enum": ["preferred_start_time", "preferred_end_time", "departure_date"]
                },
                "date": {
                    "type": "string",
                    "format": "date-time"
                }
            }
        },

        "tire_pressure": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "pressure": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/pressure" }
            }
        },

        "tire_pressure_status": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "status": {
                    "type": "string",
                    "enum": ["normal", "low", "alert"]
                }
            }
        },

        "tire_temperature": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "temperature": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/temperature" }
            }
        },

        "trip_meter": {
            "type": "object",
            "properties": {
                "id": { "type": "integer" },
                "distance": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/length" }
            }
        },

        "trouble_code": {
            "type": "object",
            "properties": {
                "occurrences": { "type": "integer" },
                "id": { "type": "string" },
                "ecu_id": { "type": "string" },
                "status": { "type": "string" },
                "system": {
                    "type": "string",
                    "enum": ["unknown", "body", "chassis", "powertrain", "network"]
                }
            }
        },

        "webhook": {
            "type": "object",
            "properties": {
                "available": {
                    "type": "string",
                    "enum": ["unavailable", "available"]
                },
                "event": { "$ref": "#/$defs/event" }
            }
        },

        "wheel_rpm": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "rpm": { "$ref": "https://highmobility.github.io/auto-api-json-schema/L13/misc/units.schema.json#/$defs/angular_velocity" }
            }
        },

        "window_open_percentage": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "open_percentage": { "type": "number" }
            }
        },

        "window_position": {
            "type": "object",
            "properties": {
                "location": { "$ref": "#/$defs/location" },
                "position": {
                    "type": "string",
                    "enum": ["closed", "open", "intermediate"]
                }
            }
        },

        "event": {
            "type": "string",
            "enum": ["ping", "trip_started", "trip_ended", "vehicle_location_changed", "authorization_changed", "tire_pressure_changed", "harsh_acceleration_triggered", "harsh_acceleration_pedal_position_triggered", "harsh_braking_triggered", "harsh_cornering_triggered", "seat_belt_triggered", "maintenance_changed", "dashboard_lights_changed", "ignition_changed", "accident_reported", "emergency_reported", "breakdown_reported", "battery_guard_warning", "engine_changed", "fleet_clearance_changed"]
        }
    }
}
