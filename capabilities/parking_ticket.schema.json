{
    "$id": "https://high-mobility.com/schemas/parking_ticket",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "status": { "$ref": "/schemas/property/status" },
        "operator_name": { "$ref": "/schemas/property/string" },
        "operator_ticket_id": { "$ref": "/schemas/property/string" },
        "ticket_start_time": { "$ref": "/schemas/property/timestamp" },
        "ticket_end_time": { "$ref": "/schemas/property/timestamp" },

        "nonce": { "$ref": "/schemas/property/nonce" },
        "vin": { "$ref": "/schemas/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "/schemas/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/failure"
                },
                "availability": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/availability"
                }
            },
            "minProperties": 1
        },


        "property_bytes": {
            "$id": "/schemas/property/bytes",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/bytes"
                }
            }
        },

        "property_nonce": {
            "$id": "/schemas/property/nonce",
            "$ref": "/schemas/property/bytes",
            "properties": {
                "data": {
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_status": {
            "$id": "/schemas/property/status,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["ended", "started"]
                }
            }
        },

        "property_string": {
            "$id": "/schemas/property/string,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string"
                }
            }
        },

        "property_timestamp": {
            "$id": "/schemas/property/timestamp,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "format": "date-time"
                }
            }
        },

        "property_vin": {
            "$id": "/schemas/property/vin",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
