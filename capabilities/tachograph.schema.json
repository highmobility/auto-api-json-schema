{
    "$id": "https://high-mobility.com/schemas/tachograph",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "drivers_working_states": { "$ref": "/schemas/property/driver_working_state" },
        "drivers_time_states": { "$ref": "/schemas/property/driver_time_state" },
        "drivers_cards_present": { "$ref": "/schemas/property/driver_card_present" },
        "vehicle_motion": { "$ref": "/schemas/property/detected" },
        "vehicle_overspeed": { "$ref": "/schemas/property/vehicle_overspeed" },
        "vehicle_direction": { "$ref": "/schemas/property/vehicle_direction" },
        "vehicle_speed": { "$ref": "/schemas/property/speed" },

        "nonce": { "$ref": "/schemas/property/nonce" },
        "vin": { "$ref": "/schemas/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "/schemas/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/failure"
                },
                "availability": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/availability"
                }
            },
            "minProperties": 1
        },


        "property_bytes": {
            "$id": "/schemas/property/bytes",
            "$ref": "/schemas/property",
            "properties": {
                "data": { "$ref": "https://gist.githubusercontent.com/doofyus/a1c290dcf40c99296c54da2fcce8962d/raw/6308e26cbf0b536a3358900f8c6009c3b8b1979c/custom_types.schema.json#/$defs/bytes" }
            }
        },

        "property_detected": {
            "$id": "/schemas/property/detected,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/detected"
                }
            }
        },

        "property_driver_card_present": {
            "$id": "/schemas/property/driver_card_present,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/driver_card_present"
                }
            }
        },

        "property_driver_time_state": {
            "$id": "/schemas/property/driver_time_state,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/driver_time_state"
                }
            }
        },

        "property_driver_working_state": {
            "$id": "/schemas/property/driver_working_state,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/driver_working_state"
                }
            }
        },

        "property_nonce": {
            "$id": "/schemas/property/nonce",
            "$ref": "/schemas/property/bytes",
            "properties": {
                "data": {
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_speed": {
            "$id": "/schemas/property/speed,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/units.schema.json#/$defs/speed"
                }
            }
        },

        "property_vehicle_direction": {
            "$id": "/schemas/property/vehicle_direction,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["forward", "reverse"]
                }
            }
        },

        "property_vehicle_overspeed": {
            "$id": "/schemas/property/vehicle_overspeed,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["no_overspeed", "overspeed"]
                }
            }
        },

        "property_vin": {
            "$id": "/schemas/property/vin",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
