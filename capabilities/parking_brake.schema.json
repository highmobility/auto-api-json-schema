{
    "$id": "https://high-mobility.com/schemas/parking_brake",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "status": { "$ref": "/schemas/property/active_state" },

        "nonce": { "$ref": "/schemas/property/nonce" },
        "vin": { "$ref": "/schemas/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "/schemas/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/failure"
                },
                "availability": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/availability"
                }
            },
            "minProperties": 1
        },


        "property_active_state": {
            "$id": "/schemas/property/active_state,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/active_state"
                }
            }
        },

        "property_bytes": {
            "$id": "/schemas/property/bytes",
            "$ref": "/schemas/property",
            "properties": {
                "data": { "$ref": "https://gist.githubusercontent.com/doofyus/a1c290dcf40c99296c54da2fcce8962d/raw/6308e26cbf0b536a3358900f8c6009c3b8b1979c/custom_types.schema.json#/$defs/bytes" }
            }
        },

        "property_nonce": {
            "$id": "/schemas/property/nonce",
            "$ref": "/schemas/property/bytes",
            "properties": {
                "data": {
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_vin": {
            "$id": "/schemas/property/vin",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
