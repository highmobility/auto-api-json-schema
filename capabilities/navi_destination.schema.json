{
    "$id": "https://high-mobility.com/schemas/navi_destination",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "coordinates": { "$ref": "/schemas/property/coordinates" },
        "destination_name": { "$ref": "/schemas/property/string" },
        "data_slots_free": { "$ref": "/schemas/property/uinteger" },
        "data_slots_max": { "$ref": "/schemas/property/uinteger" },
        "arrival_duration": { "$ref": "/schemas/property/duration" },
        "distance_to_destination": { "$ref": "/schemas/property/length" },

        "nonce": { "$ref": "/schemas/property/nonce" },
        "vin": { "$ref": "/schemas/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "/schemas/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/failure"
                },
                "availability": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/availability"
                }
            },
            "minProperties": 1
        },


        "property_bytes": {
            "$id": "/schemas/property/bytes",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/bytes"
                }
            }
        },

        "property_coordinates": {
            "$id": "/schemas/property/coordinates,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/coordinates"
                }
            }
        },

        "property_duration": {
            "$id": "/schemas/property/duration,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/units.schema.json#/$defs/duration"
                }
            }
        },

        "property_length": {
            "$id": "/schemas/property/length,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/units.schema.json#/$defs/length"
                }
            }
        },

        "property_nonce": {
            "$id": "/schemas/property/nonce",
            "$ref": "/schemas/property/bytes",
            "properties": {
                "data": {
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_string": {
            "$id": "/schemas/property/string,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string"
                }
            }
        },

        "property_uinteger": {
            "$id": "/schemas/property/uinteger,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                }
            }
        },

        "property_vin": {
            "$id": "/schemas/property/vin",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
