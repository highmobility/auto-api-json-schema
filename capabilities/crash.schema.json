{
    "$id": "https://high-mobility.com/schemas/crash",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "incidents": { "$ref": "/schemas/property/crash_incident" },
        "type": { "$ref": "/schemas/property/type" },
        "tipped_state": { "$ref": "/schemas/property/tipped_state" },
        "automatic_ecall": { "$ref": "/schemas/property/enabled_state" },
        "severity": { "$ref": "/schemas/property/uinteger" },
        "impact_zone": { "$ref": "/schemas/property/impact_zone" },

        "nonce": { "$ref": "/schemas/property/nonce" },
        "vin": { "$ref": "/schemas/property/vin" }
    },
    "additionalProperties": false,



    "$defs": {
        "property": {
            "$id": "/schemas/property",
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                },
                "failure": { "$ref": "https://gist.githubusercontent.com/doofyus/a1c290dcf40c99296c54da2fcce8962d/raw/6308e26cbf0b536a3358900f8c6009c3b8b1979c/custom_types.schema.json#/$defs/failure" },
                "availability": { "$ref": "https://gist.githubusercontent.com/doofyus/a1c290dcf40c99296c54da2fcce8962d/raw/6308e26cbf0b536a3358900f8c6009c3b8b1979c/custom_types.schema.json#/$defs/availability" }
            },
            "minProperties": 1
        },


        "property_bytes": {
            "$id": "/schemas/property/bytes",
            "$ref": "/schemas/property",
            "properties": {
                "data": { "$ref": "https://gist.githubusercontent.com/doofyus/a1c290dcf40c99296c54da2fcce8962d/raw/6308e26cbf0b536a3358900f8c6009c3b8b1979c/custom_types.schema.json#/$defs/bytes" }
            }
        },

        "property_crash_incident": {
            "$id": "/schemas/property/crash_incident,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/crash_incident"
                }
            }
        },

        "property_enabled_state": {
            "$id": "/schemas/property/enabled_state,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "$ref": "https://raw.githubusercontent.com/highmobility/auto-api-json-schema/main/misc/custom_types.schema.json#/$defs/enabled_state"
                }
            }
        },

        "property_impact_zone": {
            "$id": "/schemas/property/impact_zone,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["pedestrian_protection", "rollover", "rear_passenger_side", "rear_driver_side", "side_passeger_side", "side_driver_side", "front_passenger_side", "front_driver_side"]
                }
            }
        },

        "property_nonce": {
            "$id": "/schemas/property/nonce",
            "$ref": "/schemas/property/bytes",
            "properties": {
                "data": {
                    "minItems": 9,
                    "maxItems": 9
                }
            }
        },

        "property_tipped_state": {
            "$id": "/schemas/property/tipped_state,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["tipped_over", "not_tipped"]
                }
            }
        },

        "property_type": {
            "$id": "/schemas/property/type,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "enum": ["pedestrian", "non_pedestrian"]
                }
            }
        },

        "property_uinteger": {
            "$id": "/schemas/property/uinteger,
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                }
            }
        },

        "property_vin": {
            "$id": "/schemas/property/vin",
            "$ref": "/schemas/property",
            "properties": {
                "data": {
                    "type": "string",
                    "minLength": 17,
                    "maxLength": 17
                }
            }
        }
    }
}
